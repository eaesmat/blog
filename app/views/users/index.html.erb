<%# <h1>All Users</h1> %>
<div>
  <% @users.each do |user| %>
    <ul>
      <div class="container border rounded-3 p-3 col-8 justify-content-center">
        <div class="d-inline ">
          <img src= '<%= user.photo %>' width="100" height="100" class="" />
        </div>
        <h3 class="d-inline ps-5"><%= user.name %></h3>
        <p class="text-end">Number of Posts: <%=user.post_counter%></p>
      </div>
    </ul>
  <% end %>
</div>
